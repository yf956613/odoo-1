<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <record id="account_move_hash_view_form_wizard" model="ir.ui.view">
        <field name="name">account.move.hash.wizard.form</field>
        <field name="model">account.move.hash.wizard</field>
        <field name="arch" type="xml">
            <form string="Hash Integrity Result">
                <sheet>
                    <group>
                        <field name="hash_integrity_result" nolabel="1"/>
                    </group>
                </sheet>
                <footer>
                    <button string="Close" class="btn-primary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
