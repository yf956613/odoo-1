<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="faia_xml_data">
            <AuditFile>
                <Header>
                    <AuditFileVersion>2.01</AuditFileVersion>
                    <t t-set="company" t-value="request.env.user.company_id"/>
                    <AuditFileCountry><t t-esc="company.partner_id.country_id.code"/></AuditFileCountry>
                    <AuditFileRegion><t t-esc="company.partner_id.state_id.code"/></AuditFileRegion>
                    <AuditFileDateCreated><t t-esc="AuditFileDateCreated"/></AuditFileDateCreated>
                    <SoftwareCompanyName>Odoo SA</SoftwareCompanyName>
                    <SoftwareID>Odoo</SoftwareID>
                    <SoftwareVersion><t t-esc="SoftwareVersion"/></SoftwareVersion>
                    <Company>
                        <RegistrationNumber><t t-esc="company.company_registry"/></RegistrationNumber>
                        <Name><t t-esc="company.name"/></Name>
                        <Address>
                            <StreetName><t t-esc="company.partner_id.street"/></StreetName>
                            <AdditionalAddressDetail><t t-esc="company.partner_id.street2"/></AdditionalAddressDetail>
                            <City><t t-esc="company.partner_id.city"/></City>
                            <PostalCode><t t-esc="company.partner_id.zip"/></PostalCode>
                            <Region><t t-esc="company.partner_id.state_id.code"/></Region>
                            <Country><t t-esc="company.partner_id.country_id.code"/></Country>
                            <AddressType>ShipToAddress</AddressType>
                        </Address>
                        <Contact>
                            <ContactPerson>
                                <Title><t t-esc="company.partner_id.title.name"/></Title>
                                <FirstName><t t-esc="company.partner_id.name"/></FirstName>
                                <LastName></LastName>
                            </ContactPerson>
                            <Telephone><t t-esc="company.partner_id.mobile or company.partner_id.phone"/></Telephone>
                            <Email><t t-esc="company.partner_id.email"/></Email>
                            <Website><t t-esc="company.partner_id.website"/></Website>
                        </Contact>
                        <TaxRegistration t-if="company.vat">
                            <TaxRegistrationNumber><t t-esc="company.vat"/></TaxRegistrationNumber>
                        </TaxRegistration>
                        <BankAccount t-foreach="company.bank_ids" t-as="bank">
                            <!--You have a CHOICE of the next 2 items at this level-->
                            <BankAccountNumber><t t-esc="bank.acc_number"/></BankAccountNumber>
                            <BankAccountName><t t-esc="bank.bank_name"/></BankAccountName>
                            <SortCode><t t-esc="bank.bank_bic"/></SortCode>
                        </BankAccount>
                    </Company>
                    <DefaultCurrencyCode><t t-esc="company.currency_id.name"/></DefaultCurrencyCode>
                    <HeaderComment><t t-esc="company.report_header"/></HeaderComment>
                    <TaxAccountingBasis>Invoice Accounting</TaxAccountingBasis>
                </Header>
                <MasterFiles>
                    <GeneralLedgerAccounts t-if="accounts">
                        <Account t-foreach="accounts" t-as="account">
                            <AccountID><t t-esc="account.get('id')"/></AccountID>
                            <AccountDescription><t t-esc="account.get('name')"/></AccountDescription>
                            <StandardAccountID><t t-esc="account.get('code')"/></StandardAccountID>
                            <GroupingCategory t-if="account.get('group_code')"><t t-esc="account.get('group_code')"/></GroupingCategory>
                            <GroupingCode t-if="account.get('group_categ')"><t t-esc="account.get('group_categ')"/></GroupingCode>
                            <AccountType><t t-esc="account.get('acccount_type')"/></AccountType>
                            <t t-set="bal_from" t-value="account.get('account_balance_date_from')"/>
                            <t t-set="bal_to" t-value="account.get('account_balance_date_to')"/>
                            <OpeningDebitBalance t-if="bal_from.get('opening_debit')">
                                <t t-esc="bal_from.get('opening_debit')"/>
                            </OpeningDebitBalance>
                            <OpeningCreditBalance t-if="not bal_from.get('opening_debit')">
                                <t t-esc="bal_from.get('opening_credit') or 0.0"/>
                            </OpeningCreditBalance>
                            <ClosingDebitBalance t-if="bal_to.get('opening_debit')">
                                <t t-esc="bal_to.get('opening_debit')"/>
                            </ClosingDebitBalance>
                            <ClosingCreditBalance t-if="not bal_to.get('opening_debit')">
                                <t t-esc="bal_to.get('opening_credit') or 0.0"/>
                            </ClosingCreditBalance>
                        </Account>
                    </GeneralLedgerAccounts>
                    <Customers t-if="customers">
                        <Customer t-foreach="customers" t-as="Customer">
                            <Name><t t-esc="Customer.get('name')"/></Name>
                            <Address>
                                <StreetName><t t-esc="Customer.get('address').get('cust_street')"/></StreetName>
                                <AdditionalAddressDetail><t t-esc="Customer.get('address').get('cust_add_address')"/></AdditionalAddressDetail>
                                <City><t t-esc="Customer.get('address').get('city')"/></City>
                                <PostalCode><t t-esc="Customer.get('address').get('postal_code')"/></PostalCode>
                                <Region><t t-esc="Customer.get('address').get('region')"/></Region>
                                <Country><t t-esc="Customer.get('address').get('country')"/></Country>
                                <AddressType>PostalAddress</AddressType>
                            </Address>
                            <Address t-foreach="Customer.get('address').get('other_address')" t-as="address">
                                <StreetName><t t-esc="address.get('cust_street')"/></StreetName>
                                <AdditionalAddressDetail><t t-esc="address.get('cust_add_address')"/></AdditionalAddressDetail>
                                <City><t t-esc="address.get('city')"/></City>
                                <PostalCode><t t-esc="address.get('postal_code')"/></PostalCode>
                                <Region><t t-esc="address.get('region')"/></Region>
                                <Country><t t-esc="address.get('country')"/></Country>
                                <AddressType>PostalAddress</AddressType>
                            </Address>
                            <Contact t-foreach="Customer.get('address').get('contact_address')" t-as="contact">
                                <ContactPerson>
                                    <Title><t t-esc="contact.get('title')"/></Title>
                                    <FirstName><t t-esc="contact.get('name')"/></FirstName>
                                    <LastName></LastName>
                                </ContactPerson>
                                <Telephone><t t-esc="contact.get('mobile') or contact.get('phone')"/></Telephone>
                                <Email><t t-esc="contact.get('email')"/></Email>
                                <Website><t t-esc="contact.get('website')"/></Website>
                            </Contact>
                            <TaxRegistration t-if="Customer.get('tax_registration')">
                                <TaxRegistrationNumber><t t-esc="Customer.get('tax_registration')"/></TaxRegistrationNumber>
                            </TaxRegistration>
                            <BankAccount t-foreach="Customer.get('bank_accounts')" t-as="bank">
                                <!--You have a CHOICE of the next 2 items at this level-->
                                <BankAccountNumber><t t-esc="bank.get('number')"/></BankAccountNumber>
                                <BankAccountName><t t-esc="bank.get('name')"/></BankAccountName>
                                <SortCode><t t-esc="bank.get('code')"/></SortCode>
                            </BankAccount>
                            <CustomerID><t t-esc="Customer.get('id')"/></CustomerID>
                            <t t-set="customer_bal_from" t-value="Customer.get('partner_balance_date_from')"/>
                            <OpeningDebitBalance t-if="customer_bal_from.get('debit')">
                                <t t-esc="customer_bal_from.get('debit')"/>
                            </OpeningDebitBalance>
                            <OpeningCreditBalance t-if="customer_bal_from.get('credit')">
                                <t t-esc="customer_bal_from.get('credit')"/>
                            </OpeningCreditBalance>
                            <t t-set="customer_bal_to" t-value="Customer.get('partner_balance_date_to')"/>
                            <ClosingDebitBalance t-if="customer_bal_to.get('debit')">
                                <t t-esc="customer_bal_to.get('debit')"/>
                            </ClosingDebitBalance>
                            <ClosingCreditBalance t-if="customer_bal_to.get('credit')">
                                <t t-esc="customer_bal_to.get('credit')"/>
                            </ClosingCreditBalance>
                        </Customer>
                    </Customers>
                    <Suppliers t-if="suppliers">
                        <Supplier t-foreach="suppliers" t-as="Supplier">
                            <Name><t t-esc="Supplier.get('name')"/></Name>
                            <Address>
                                <StreetName><t t-esc="Supplier.get('address').get('cust_street')"/></StreetName>
                                <AdditionalAddressDetail><t t-esc="Supplier.get('address').get('cust_add_address')"/></AdditionalAddressDetail>
                                <City><t t-esc="Supplier.get('address').get('city')"/></City>
                                <PostalCode><t t-esc="Supplier.get('address').get('postal_code')"/></PostalCode>
                                <Region><t t-esc="Supplier.get('address').get('region')"/></Region>
                                <Country><t t-esc="Supplier.get('address').get('country')"/></Country>
                                <AddressType>BillingAddress</AddressType>
                            </Address>
                            <Address t-foreach="Supplier.get('address').get('other_address')" t-as="address">
                                <StreetName><t t-esc="address.get('cust_street')"/></StreetName>
                                <AdditionalAddressDetail><t t-esc="address.get('cust_add_address')"/></AdditionalAddressDetail>
                                <City><t t-esc="address.get('city')"/></City>
                                <PostalCode><t t-esc="address.get('postal_code')"/></PostalCode>
                                <Region><t t-esc="address.get('region')"/></Region>
                                <Country><t t-esc="address.get('country')"/></Country>
                                <AddressType>BillingAddress</AddressType>
                            </Address>
                            <Contact t-foreach="Supplier.get('address').get('contact_address')" t-as="contact">
                                <ContactPerson>
                                    <Title><t t-esc="contact.get('title')"/></Title>
                                    <FirstName><t t-esc="contact.get('name')"/></FirstName>
                                    <LastName></LastName>
                                </ContactPerson>
                                <Telephone><t t-esc="contact.get('mobile') or contact.get('phone')"/></Telephone>
                                <Email><t t-esc="contact.get('email')"/></Email>
                                <Website><t t-esc="contact.get('website')"/></Website>
                            </Contact>
                            <TaxRegistration t-if="Supplier.get('tax_registration')">
                                <TaxRegistrationNumber><t t-esc="Supplier.get('tax_registration')"/></TaxRegistrationNumber>
                            </TaxRegistration>
                            <BankAccount t-foreach="Supplier.get('bank_accounts')" t-as="bank">
                                <!--You have a CHOICE of the next 2 items at this level-->
                                <BankAccountNumber><t t-esc="bank.get('number')"/></BankAccountNumber>
                                <BankAccountName><t t-esc="bank.get('name')"/></BankAccountName>
                                <SortCode><t t-esc="bank.get('code')"/></SortCode>
                            </BankAccount>
                            <SupplierID><t t-esc="Supplier.get('id')"/></SupplierID>
                            <t t-set="supplier_bal_from" t-value="Supplier.get('partner_balance_date_from')"/>
                            <OpeningDebitBalance t-if="supplier_bal_from.get('debit')">
                                <t t-esc="supplier_bal_from.get('debit')"/>
                            </OpeningDebitBalance>
                            <OpeningCreditBalance t-if="supplier_bal_from.get('credit')">
                                <t t-esc="supplier_bal_from.get('credit')"/>
                            </OpeningCreditBalance>
                            <t t-set="supplier_bal_to" t-value="Supplier.get('partner_balance_date_to')"/>
                            <ClosingDebitBalance t-if="supplier_bal_to.get('debit')">
                                <t t-esc="supplier_bal_to.get('debit')"/>
                            </ClosingDebitBalance>
                            <ClosingCreditBalance t-if="supplier_bal_to.get('credit')">
                                <t t-esc="supplier_bal_to.get('credit')"/>
                            </ClosingCreditBalance>
                        </Supplier>
                    </Suppliers>
                    <TaxTable t-if="taxes">
                        <TaxTableEntry t-foreach="taxes" t-as="TaxTableEntry">
                            <TaxType><t t-esc="TaxTableEntry.get('tax_type')"/></TaxType>
                            <Description>Taxe sur la valeur ajout&amp;#xC3;&amp;#xA9;e</Description>
                            <TaxCodeDetails>
                                <TaxCode><t t-esc="TaxTableEntry.get('tax_code')"/></TaxCode>
                                <Description><t t-esc="TaxTableEntry.get('description')"/></Description>
                                <!--You have a CHOICE of the next 2 items at this level-->
                                <TaxPercentage t-if="not TaxTableEntry.get('fix_amount')"><t t-esc="TaxTableEntry.get('percent_amount') or 0.0" /></TaxPercentage>
                                <FlatTaxRate t-if="TaxTableEntry.get('fix_amount')">
                                    <Amount><t t-esc="TaxTableEntry.get('fix_amount')" /></Amount>
                                    <CurrencyCode t-if="TaxTableEntry.get('currency_code')"><t t-esc="TaxTableEntry.get('currency_code')"/></CurrencyCode>
                                    <CurrencyAmount><t t-esc="TaxTableEntry.get('currency_amount')" /></CurrencyAmount>
                                </FlatTaxRate>
                                <Country><t t-esc="TaxTableEntry.get('country')"/></Country>
                                <Region><t t-esc="TaxTableEntry.get('region')"/></Region>
                            </TaxCodeDetails>
                        </TaxTableEntry>
                    </TaxTable>
                    <UOMTable t-if="uom_entries">
                        <UOMTableEntry t-foreach="uom_entries" t-as="UOMTableEntry">
                            <UnitOfMeasure><t t-esc="UOMTableEntry.get('measure')"/></UnitOfMeasure>
                            <Description><t t-esc="UOMTableEntry.get('description')"/></Description>
                        </UOMTableEntry>
                    </UOMTable>
                    <Products t-if="products">
                        <Product t-foreach="products" t-as="Product">
                            <ProductCode><t t-esc="Product.get('product_code')"/></ProductCode>
                            <ProductGroup><t t-esc="Product.get('group')"/></ProductGroup>
                            <Description><t t-esc="Product.get('description')"/></Description>
                            <ProductNumberCode><t t-esc="Product.get('product_number_code')"/></ProductNumberCode>
                            <ValuationMethod><t t-esc="Product.get('valuation')"/></ValuationMethod>
                            <UOMBase><t t-esc="Product.get('uom_base')"/></UOMBase>
                            <UOMStandard><t t-esc="Product.get('uom_standard')"/></UOMStandard>
                            <UOMToUOMBaseConversionFactor><t t-esc="Product.get('convert_fact')"/></UOMToUOMBaseConversionFactor>
                            <!--Zero or more repetitions:-->
                            <Tax t-foreach="Product.get('taxes')" t-as="tax">
                                <TaxType><t t-esc="tax.get('type')"/></TaxType>
                                <TaxCode><t t-esc="tax.get('code')"/></TaxCode>
                            </Tax>
                        </Product>
                    </Products>
                    <Owners>
                        <Owner>
                            <Name><t t-esc="company.name"/></Name>
                            <Address>
                                <StreetName><t t-esc="owner_address.get('address').get('cust_street')"/></StreetName>
                                <AdditionalAddressDetail><t t-esc="owner_address.get('address').get('cust_add_address')"/></AdditionalAddressDetail>
                                <City><t t-esc="owner_address.get('address').get('city')"/></City>
                                <PostalCode><t t-esc="owner_address.get('address').get('postal_code')"/></PostalCode>
                                <Region><t t-esc="owner_address.get('address').get('region')"/></Region>
                                <Country><t t-esc="owner_address.get('address').get('country')"/></Country>
                                <AddressType>BillingAddress</AddressType>
                            </Address>
                            <Address t-foreach="owner_address.get('address').get('other_address')" t-as="Address">
                                <StreetName><t t-esc="Address.get('cust_street')"/></StreetName>
                                <AdditionalAddressDetail><t t-esc="Address.get('cust_add_address')"/></AdditionalAddressDetail>
                                <City><t t-esc="Address.get('city')"/></City>
                                <PostalCode><t t-esc="Address.get('postal_code')"/></PostalCode>
                                <Region><t t-esc="Address.get('region')"/></Region>
                                <Country><t t-esc="Address.get('country')"/></Country>
                                <AddressType>BillingAddress</AddressType>
                            </Address>
                            <Contact t-foreach="owner_address.get('address').get('contact_address')" t-as="Contact">
                                <ContactPerson>
                                    <Title t-if="Contact.get('title')"><t t-esc="Contact.get('title')"/></Title>
                                    <FirstName><t t-esc="Contact.get('name')"/></FirstName>
                                    <LastName></LastName>
                                </ContactPerson>
                                <Telephone><t t-esc="Contact.get('phone')"/></Telephone>
                                <Email><t t-esc="Contact.get('email')"/></Email>
                                <Website><t t-esc="Contact.get('website')"/></Website>
                            </Contact>
                            <TaxRegistration t-if="company.vat">
                                <TaxRegistrationNumber><t t-esc="company.vat"/></TaxRegistrationNumber>
                            </TaxRegistration>
                            <BankAccount t-foreach="company.bank_ids" t-as="bank">
                                <!--You have a CHOICE of the next 2 items at this level-->
                                <BankAccountNumber><t t-esc="bank.acc_number"/></BankAccountNumber>
                                <BankAccountName><t t-esc="bank.bank_name"/></BankAccountName>
                                <SortCode><t t-esc="bank.bank_bic"/></SortCode>
                            </BankAccount>
                            <OwnerID><t t-esc="company.id"/></OwnerID>
                        </Owner>
                    </Owners>
                </MasterFiles>
                <GeneralLedgerEntries>
                    <NumberOfEntries><t t-esc="len(general_ledger.get('journals'))"/></NumberOfEntries>
                    <TotalDebit><t t-esc="general_ledger.get('total_debit')"/></TotalDebit>
                    <TotalCredit><t t-esc="general_ledger.get('total_credit')"/></TotalCredit>
                    <Journal t-foreach="general_ledger.get('journals')" t-as="journal">
                        <JournalID><t t-esc="journal.get('journal_id')"/></JournalID>
                        <Description><t t-esc="journal.get('description')"/></Description>
                        <Type><t t-esc="journal.get('type')"/></Type>
                        <Transaction t-foreach="journal.get('transactions')" t-as="move">
                            <TransactionID><t t-esc="move.get('transaction_id')"/></TransactionID>
                            <Period><t t-esc="move.get('period')"/></Period>
                            <PeriodYear><t t-esc="move.get('period_year')"/></PeriodYear>
                            <TransactionDate><t t-esc="move.get('transaction_date')"/></TransactionDate>
                            <SourceID><t t-esc="move.get('source_id')"/></SourceID>
                            <TransactionType><t t-esc="move.get('transaction_type')"/></TransactionType>
                            <Description><t t-esc="move.get('description')"/></Description>
                            <SystemEntryDate><t t-esc="move.get('system_entry_date')"/></SystemEntryDate>
                            <GLPostingDate><t t-esc="move.get('glposting_date')"/></GLPostingDate>
                            <CustomerID t-if="move.get('customer_id')"><t t-esc="move.get('customer_id')"/></CustomerID>
                            <SupplierID t-if="move.get('supplier_id')"><t t-esc="move.get('supplier_id')"/></SupplierID>
                            <Line t-foreach="move.get('lines')" t-as="line">
                                <RecordID><t t-esc="line.get('record_id')"/></RecordID>
                                <AccountID><t t-esc="line.get('account_id')"/></AccountID>
                                <ValueDate><t t-esc="line.get('valuedate')"/></ValueDate>
                                <SourceDocumentID><t t-esc="line.get('sourcedocument_id')"/></SourceDocumentID>
                                <CustomerID t-if="move.get('customer_id')"><t t-esc="line.get('customer_id')"/></CustomerID>
                                <SupplierID t-if="move.get('supplier_id')"><t t-esc="line.get('supplier_id')"/></SupplierID>
                                <Description><t t-esc="line.get('description')"/></Description>
                                <DebitAmount t-if="line.get('debit_amount')">
                                    <Amount><t t-esc="line.get('debit_amount').get('amount')"/></Amount>
                                    <CurrencyCode><t t-esc="line.get('debit_amount').get('currency_code')"/></CurrencyCode>
                                    <CurrencyAmount><t t-esc="line.get('debit_amount').get('currency_amount')"/></CurrencyAmount>
                                </DebitAmount>
                                <CreditAmount t-if="line.get('credit_amount')">
                                    <Amount><t t-esc="line.get('credit_amount').get('amount')"/></Amount>
                                    <CurrencyCode><t t-esc="line.get('credit_amount').get('currency_code')"/></CurrencyCode>
                                    <CurrencyAmount><t t-esc="line.get('credit_amount').get('currency_amount')"/></CurrencyAmount>
                                </CreditAmount>
                                <TaxInformation t-foreach="line.get('tax_information')" t-as="tax">
                                    <TaxType><t t-esc="tax.get('tax_type')"/></TaxType>
                                    <TaxCode><t t-esc="tax.get('tax_code')"/></TaxCode>
                                    <TaxPercentage><t t-esc="tax.get('tax_percentage')" /></TaxPercentage>
                                    <TaxBaseDescription><t t-esc="tax.get('name')"/></TaxBaseDescription>
                                    <TaxAmount>
                                        <Amount><t t-esc="tax.get('amount')" /></Amount>
                                        <CurrencyCode t-if="tax.get('currency_code')"><t t-esc="tax.get('currency_code')"/></CurrencyCode>
                                        <CurrencyAmount><t t-esc="tax.get('currency_amount')" /></CurrencyAmount>
                                    </TaxAmount>
                                </TaxInformation>
                            </Line>
                        </Transaction>
                    </Journal>
                </GeneralLedgerEntries>
                <SourceDocuments>
                    <SalesInvoices>
                        <NumberOfEntries><t t-esc="len(sales_invoices.get('invoices'))"/></NumberOfEntries>
                        <TotalDebit><t t-esc="sales_invoices.get('total_debit')"/></TotalDebit>
                        <TotalCredit><t t-esc="sales_invoices.get('total_credit')"/></TotalCredit>
                        <Invoice t-foreach="sales_invoices.get('invoices')" t-as="invoice">
                            <InvoiceNo><t t-esc="invoice.get('invoice_no')"/></InvoiceNo>
                            <CustomerInfo t-if="invoice.get('customer_info')">
                                <CustomerID><t t-esc="invoice.get('partner_id')"/></CustomerID>
                                <BillingAddress>
                                    <StreetName><t t-esc="invoice.get('customer_info').get('cust_street')"/></StreetName>
                                    <AdditionalAddressDetail><t t-esc="invoice.get('customer_info').get('cust_add_address')"/></AdditionalAddressDetail>
                                    <City><t t-esc="invoice.get('customer_info').get('city')"/></City>
                                    <PostalCode><t t-esc="invoice.get('customer_info').get('postal_code')"/></PostalCode>
                                    <Region><t t-esc="invoice.get('customer_info').get('region')"/></Region>
                                    <Country><t t-esc="invoice.get('customer_info').get('country')"/></Country>
                                    <AddressType>PostalAddress</AddressType>
                                </BillingAddress>
                            </CustomerInfo>
                            <SupplierInfo t-if="invoice.get('supplier_info')">
                                <SupplierID><t t-esc="invoice.get('partner_shipping_id')"/></SupplierID>
                                <BillingAddress>
                                    <StreetName><t t-esc="invoice.get('customer_info').get('cust_street')"/></StreetName>
                                    <AdditionalAddressDetail><t t-esc="invoice.get('customer_info').get('cust_add_address')"/></AdditionalAddressDetail>
                                    <City><t t-esc="invoice.get('customer_info').get('city')"/></City>
                                    <PostalCode><t t-esc="invoice.get('customer_info').get('postal_code')"/></PostalCode>
                                    <Region><t t-esc="invoice.get('customer_info').get('region')"/></Region>
                                    <Country><t t-esc="invoice.get('customer_info').get('country')"/></Country>
                                    <AddressType>BillingAddress</AddressType>
                                </BillingAddress>
                            </SupplierInfo>
                            <AccountID><t t-esc="invoice.get('account_id')"/></AccountID>
                            <Period><t t-esc="invoice.get('period')"/></Period>
                            <PeriodYear><t t-esc="invoice.get('period_year')"/></PeriodYear>
                            <InvoiceDate><t t-esc="invoice.get('invoice_date')"/></InvoiceDate>
                            <InvoiceType><t t-esc="invoice.get('invoice_type')"/></InvoiceType>
                            <ShipTo t-if="invoice.get('ship_to_address')">
                                <DeliveryDate><t t-esc="invoice.get('ship_to_delivery_date')"/></DeliveryDate>
                                <Address t-if="invoice.get('ship_to_address')">
                                    <StreetName><t t-esc="invoice.get('ship_to_address').get('cust_street')"/></StreetName>
                                    <AdditionalAddressDetail><t t-esc="invoice.get('ship_to_address').get('cust_add_address')"/></AdditionalAddressDetail>
                                    <City><t t-esc="invoice.get('ship_to_address').get('city')"/></City>
                                    <PostalCode><t t-esc="invoice.get('ship_to_address').get('postal_code')"/></PostalCode>
                                    <Region><t t-esc="invoice.get('ship_to_address').get('region')"/></Region>
                                    <Country><t t-esc="invoice.get('ship_to_address').get('country')"/></Country>
                                    <AddressType>ShipFromAddress</AddressType>
                                </Address>
                            </ShipTo>
                            <ShipFrom>
                                <DeliveryDate><t t-esc="invoice.get('ship_from_delivery_date')"/></DeliveryDate>
                                <Address>
                                    <StreetName><t t-esc="invoice.get('ship_from_address').get('cust_street')"/></StreetName>
                                    <AdditionalAddressDetail><t t-esc="invoice.get('ship_from_address').get('cust_add_address')"/></AdditionalAddressDetail>
                                    <City><t t-esc="invoice.get('ship_from_address').get('city')"/></City>
                                    <PostalCode><t t-esc="invoice.get('ship_from_address').get('postal_code')"/></PostalCode>
                                    <Region><t t-esc="invoice.get('ship_from_address').get('region')"/></Region>
                                    <Country><t t-esc="invoice.get('ship_from_address').get('country')"/></Country>
                                    <AddressType>BillingAddress</AddressType>
                                </Address>
                            </ShipFrom>
                            <PaymentTerms><t t-esc="invoice.get('payment_term_id')"/></PaymentTerms>
                            <SourceID t-if="invoice.get('source_id')"><t t-esc="invoice.get('source_id')"/></SourceID>
                            <GLPostingDate><t t-esc="invoice.get('gl_posting_date')"/></GLPostingDate>
                            <TransactionID><t t-esc="invoice.get('transaction_id')"/></TransactionID>
                            <ReceiptNumbers t-if="invoice.get('receipt_number')"><t t-esc="invoice.get('receipt_number')"/></ReceiptNumbers>
                            <Line t-foreach="invoice.get('lines')" t-as="line">
                                <LineNumber><t t-esc="line.get('line_number')"/></LineNumber>
                                <AccountID><t t-esc="line.get('account_id')"/></AccountID>
                                <OrderReferences>
                                    <OriginatingON><t t-esc="line.get('order_ref')"/></OriginatingON>
                                </OrderReferences>
                                <ShipTo>
                                    <DeliveryDate><t t-esc="line.get('ship_delivery_date')"/></DeliveryDate>
                                    <Address>
                                        <StreetName><t t-esc="line.get('ship_to_address').get('cust_street')"/></StreetName>
                                        <AdditionalAddressDetail><t t-esc="line.get('ship_to_address').get('cust_add_address')"/></AdditionalAddressDetail>
                                        <City><t t-esc="line.get('ship_to_address').get('city')"/></City>
                                        <PostalCode><t t-esc="line.get('ship_to_address').get('postal_code')"/></PostalCode>
                                        <Region><t t-esc="line.get('ship_to_address').get('region')"/></Region>
                                        <Country><t t-esc="line.get('ship_to_address').get('country')"/></Country>
                                        <AddressType>StreetAddress</AddressType>
                                    </Address>
                                </ShipTo>
                                <ShipFrom>
                                    <DeliveryDate><t t-esc="line.get('ship_delivery_date')"/></DeliveryDate>
                                    <Address>
                                        <StreetName><t t-esc="line.get('ship_from_address').get('cust_street')"/></StreetName>
                                        <AdditionalAddressDetail><t t-esc="line.get('ship_from_address').get('cust_add_address')"/></AdditionalAddressDetail>
                                        <City><t t-esc="line.get('ship_from_address').get('city')"/></City>
                                        <PostalCode><t t-esc="line.get('ship_from_address').get('postal_code')"/></PostalCode>
                                        <Region><t t-esc="line.get('ship_from_address').get('region')"/></Region>
                                        <Country><t t-esc="line.get('ship_from_address').get('country')"/></Country>
                                        <AddressType>ShipFromAddress</AddressType>
                                    </Address>
                                </ShipFrom>
                                <ProductCode t-if="line.get('product_code')"><t t-esc="line.get('product_code')"/></ProductCode>
                                <ProductDescription><t t-esc="line.get('product_desc')"/></ProductDescription>
                                <Delivery>
                                    <DeliveryDate><t t-esc="line.get('delivery_date')"/></DeliveryDate>
                                </Delivery>
                                <Quantity><t t-esc="line.get('quantity')"/></Quantity>
                                <InvoiceUOM t-if="line.get('uom_id')"><t t-esc="line.get('uom_id')"/></InvoiceUOM>
                                <UOMToUOMBaseConversionFactor><t t-esc="line.get('convert_fact')"/></UOMToUOMBaseConversionFactor>
                                <UnitPrice><t t-esc="line.get('unit_price')" /></UnitPrice>
                                <TaxPointDate><t t-esc="line.get('to_point_date')"/></TaxPointDate>
                                <References>
                                    <CreditNote t-foreach="line.get('credit_note')" t-as="creditnote">
                                        <Reference t-if="creditnote.get('reference')"><t t-esc="creditnote.get('reference')"/></Reference>
                                        <Reason><t t-esc="creditnote.get('reason')"/></Reason>
                                    </CreditNote>
                                </References>
                                <Description><t t-esc="line.get('description')"/></Description>
                                <InvoiceLineAmount>
                                    <Amount><t t-esc="line.get('amount')"/></Amount>
                                    <CurrencyCode t-if="line.get('currency_code')"><t t-esc="line.get('currency_code')"/></CurrencyCode>
                                    <CurrencyAmount t-if="line.get('currency_amount')"><t t-esc="line.get('currency_amount')"/></CurrencyAmount>
                                    <ExchangeRate t-if="line.get('exchange_rate')"><t t-esc="line.get('exchange_rate')"/></ExchangeRate>
                                </InvoiceLineAmount>
                                <DebitCreditIndicator><t t-esc="line.get('indicator')"/></DebitCreditIndicator>
                            </Line>
                            <DocumentTotals>
                                <TaxInformationTotals t-foreach="invoice.get('tax_information')" t-as="tax">
                                    <TaxType><t t-esc="tax.get('tax_type')"/></TaxType>
                                    <TaxCode><t t-esc="tax.get('tax_code')"/></TaxCode>
                                    <TaxPercentage><t t-esc="tax.get('tax_percentage')" /></TaxPercentage>
                                    <TaxBaseDescription><t t-esc="tax.get('tax_description')"/></TaxBaseDescription>
                                    <TaxAmount>
                                        <Amount><t t-esc="tax.get('tax_amount')"/></Amount>
                                        <CurrencyCode t-if="tax.get('currency_code')"><t t-esc="tax.get('currency_code')"/></CurrencyCode>
                                        <CurrencyAmount><t t-esc="tax.get('currency_amount')"/></CurrencyAmount>
                                        <ExchangeRate t-if="tax.get('exchange_rate')"><t t-esc="tax.get('exchange_rate')"/></ExchangeRate>
                                    </TaxAmount>
                                </TaxInformationTotals>
                                <NetTotal><t t-esc="invoice.get('net_total')" /></NetTotal>
                                <GrossTotal><t t-esc="invoice.get('gross_total')" /></GrossTotal>
                            </DocumentTotals>
                        </Invoice>
                    </SalesInvoices>
                </SourceDocuments>
            </AuditFile>
        </template>
    </data>
