<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_lu_faia_view1" model="ir.ui.view">
        <field name="name">account.lu.faia.form.view</field>
        <field name="model">account.lu.faia1</field>
        <field name="arch" type="xml">
            <form string="FAIA File Generation">
                <group>
                    <field name="date_from"/>
                    <field name="date_to"/>
                </group>
                <footer>
                    <button string="Generate" name="generate_faia_report" type="object"
                        class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="account_lu_faia_action1" model="ir.actions.act_window">
        <field name="name">FAIA111111</field>
        <field name="res_model">account.lu.faia1</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <menuitem id="account_lu_faia_menu1"
            parent="l10n_lu.account_reports_lu_statements_menu"
            action="account_lu_faia_action1"
            sequence="99" />

</odoo>